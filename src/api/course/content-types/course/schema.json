{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "fee": {
      "type": "integer"
    },
    "isFull": {
      "type": "boolean"
    },
    "openRegisterDate": {
      "type": "date"
    },
    "endRegisterDate": {
      "type": "date"
    },
    "participantNumber": {
      "type": "integer"
    },
    "isOpen": {
      "type": "boolean"
    },
    "instructors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::instructor.instructor"
    },
    "registrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::registration.registration",
      "mappedBy": "course"
    },
    "course_info": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::course-info.course-info",
      "mappedBy": "course"
    },
    "isOnline": {
      "type": "boolean"
    },
    "venue": {
      "type": "string"
    },
    "topBanner": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
