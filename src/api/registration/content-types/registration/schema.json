{
  "kind": "collectionType",
  "collectionName": "registrations",
  "info": {
    "singularName": "registration",
    "pluralName": "registrations",
    "displayName": "Registration"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "registrations"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "registerDate": {
      "type": "datetime"
    },
    "registerStatus": {
      "type": "enumeration",
      "enum": [
        "REGISTERED",
        "PAYMENT_PENDING",
        "PAYMENT_RECEIVED",
        "ENROLLED",
        "WAIT_LIST"
      ]
    },
    "uploads": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "receipt": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
